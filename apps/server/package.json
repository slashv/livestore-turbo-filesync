{
  "name": "@repo/server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy:preview": "./scripts/deploy.sh preview",
    "deploy:prod": "./scripts/deploy.sh prod",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply livestore-auth-prod --local",
    "db:migrate:dev": "wrangler d1 migrations apply livestore-auth-dev --remote --env dev",
    "db:migrate:prod": "wrangler d1 migrations apply livestore-auth-prod --remote"
  },
  "dependencies": {
    "@better-auth/expo": "^1.2.0",
    "@livestore/sync-cf": "catalog:",
    "better-auth": "^1.2.0",
    "drizzle-orm": "^0.38.0",
    "hono": "^4.6.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "@repo/tsconfig": "workspace:*",
    "drizzle-kit": "^0.30.0",
    "typescript": "^5.7.2",
    "wrangler": "^4.59.1"
  }
}
